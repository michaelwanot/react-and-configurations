{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": [2],
    "quotes": ["error", "single"],
    "eqeqeq": ["error", "always"],
    "no-unused-vars": "error",
    "block-spacing": ["error", "never"],
    "comma-spacing": ["error", { "before": false, "after": true }],
    "eol-last": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "lines-between-class-members": ["error", "always"]
  }
}